{
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "extends": [ "eslint:recommended", "plugin:react/recommended" ],
    "rules": {
        "react/prop-types": "off",
        "react/jsx-no-target-blank": "off"
    }
}
